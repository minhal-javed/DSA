    "def factorial(n):\n",
    "    if n==0:\n",
    "        return 1\n",
    "    else:\n",
    "        return factorial(n-1)*n\n",
    "\n",
    "n=int(input(\"enter a number \"))\n",
    "fact=factorial(n)\n",
    "print(fact)"
